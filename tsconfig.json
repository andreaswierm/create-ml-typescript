{
  "$schema": "http://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "typeRoots": ["./node_modules/@types"],
    "target": "es5",
    "strict": true,
    "sourceMap": false,
    "moduleResolution": "node",
    "module": "commonjs",
    "lib": ["es6"],
    "esModuleInterop": true,
    "downlevelIteration": true,
    "declaration": true,
    "allowJs": false
  },
  "paths": {
    "@create-ml-typescript/cli": ["../cli/src"],
    "@create-ml-typescript/scripts": ["../scripts/src"]
  }
}
